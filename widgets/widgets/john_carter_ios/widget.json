{
	      "version" : "0.0.1"
	,        "name" : "Widget Foo"
	, "description" : "Some widget"
	,      "author" : "Razorfish"
	,       "email" : "nobody@razorfish.com"
	,         "uri" : "http://www.razorfish.com"

	, "jqueryPlugins" : [
		  "string"
		, "mvc/view/mutable"
		, "mobile-webkit/viewport-size"
		, "mobile-webkit/orientation"
		, "sprite-animation"
		, "3d-files/dot-mtl"
		, "3d-files/dot-obj"
		, "3d-model"
		, "css3d/environment"
		, "css3d/cylinder"
		, "util/image-loader"
	]

	,  "minJQueryVersion" : "1.6.0"
	, "testJQueryVersion" : "1.6.4"

	, "files" : [
		  "resources/models.json"
		, "js/normal.js"
	]

	, "views" : {
		"index" : {
			"files" : "js/widget.js"
		}

		, "model-viewer" : {
			"files" : [
				  "js/model-viewer.js"
				, "css/model-viewer.css"
			]
		}
	}

	, "settings" : {
		"size" : {
			     "text" : "Select widget size"
			,    "type" : "select"
			, "default" : "small"
			, "options" : {
				   "tall" : "Tall (150x300)"
				,  "wide" : "Wide (300x150)"
				, "small" : "Small (200x200)"
				, "large" : "Large (350x350)"
			}
		}
	}
}