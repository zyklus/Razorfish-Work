border-radius()
	-webkit-border-radius arguments
	-moz-border-radius    arguments
	border-radius         arguments

body a
	font             12px/1.4 "Lucida Grande", Arial, sans-serif
	background       sprite( '../images/sprites/twitter.png' )
	color            #cccddd
	border-radius    10px

form input
	padding       5px
	border        1px solid
	border-radius 5px

.block-elm {
	background : sprite( '../images/sprites/razorfish.png' ) no-repeat;
	width      : 25px;
	height     : 25px;
}